        "def abundant(limit):\n",
        "  nums = [12]\n",
        "  n = 0\n",
        "  while nums[-1] < limit:\n",
        "    if get_divisors(n+1) > n+1:\n",
        "      nums.append(n+1)\n",
        "    n += 1\n",
        "  del nums[0]\n",
        "  return nums\n",
        "\n",
        "\n",
        "ab = (abundant(28123))\n",
