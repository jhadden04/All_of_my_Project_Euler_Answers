        "#problem 53\n",
        "def nCk(n,k):\n",
        "    m=0\n",
        "    if k==0:\n",
        "        m=1\n",
        "    if k==1:\n",
        "        m=n\n",
        "    if k>=2:\n",
        "        num,dem,op1,op2=1,1,k,n\n",
        "        while(op1>=1):\n",
        "            num*=op2\n",
        "            dem*=op1\n",
        "            op1-=1\n",
        "            op2-=1\n",
        "        m=num//dem\n",
        "    return m\n",
        "total_big = 0\n",
        "for i in range(0,101):\n",
        "  for j in range(i):\n",
        "    comb = (nCk(i,j))\n",
        "    if comb > 1000000:\n",
        "      total_big += 1\n",
        "print(total_big)"
